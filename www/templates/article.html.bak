<ion-view view-title="Articulo">
  <ion-content>
    <h1>DALE</h1>
    <ion-list>
      <ion-item class="item-remove-animate item-avatar item-icon-right" ng-repeat="playlist in playlists.featured" type="item-text-wrap" href="#/tab/articulo/{{playlist.id}}">
        <h2>{{playlist.data.title}}</h2>
      </ion-item>
    </ion-list> 
  </ion-content>
</ion-view>
